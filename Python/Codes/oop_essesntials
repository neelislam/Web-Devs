class Student:
    first_Name = ""
    last_Name = ""
    def get_full_name(self):
        print(f"{self.first_Name} {self.last_Name}")

st1 = Student()    
st2 = Student()



st1.first_Name = "John"
st2.first_Name = "Doe"

print(st1.first_Name)
print(st2.first_Name)

st1.get_full_name()
st2.get_full_name()