fruits = ["mango", "banana", "orange", "grape", "kiwi", "watermelon", "pineapple"]


items = [True, False, 3, 5.6, "Hello", 7+8j]

print(fruits)
print(items[5])

fruits[2] = "apple"
print(fruits)
print("Index 2 is updated")
print("Size of the list is:")
print(len(fruits)) 

print(fruits[1:3]) #slicing
print(fruits[2:]) #slicing
print(fruits[:4]) #slicing
print(fruits[-1]) #last item
print(fruits[3:6]) #slicing

#Mutable vs Immutable
#List is mutable
#Tuple is immutable




fruits.pop()
fruits.remove("banana")
fruits.append("cherry")
fruits.insert(1, "blueberry")
fruits.sort()
fruits.reverse()
print(fruits)


students = {
    "name" : "Jerin Sneha",
    "age" : 22,
    "gender" : "Female"

}

dictionary_items = students.items()
print(dictionary_items)
print(students.keys())
print(students.values())
print(students.get("age"))
students["age"] = 21
print(students)



for fruit in fruits:
    print(fruit)